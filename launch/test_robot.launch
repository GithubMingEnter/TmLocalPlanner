<launch>

    <!-- 启动二维地图 -->
    <include file="$(find topp2)/launch/map_creater.launch" />

    <!--图搜索 算法-->
    <node pkg="topp2" type="topp_robot_sim" name="topp_robot_sim" output="screen">
        <param name="robot_mode" value="0"/>
        <param name="robot_length" value="0.4" />
        <param name="robot_width"  value="0.3" />
        <param name="robot_height" value="0.4" />
    </node>
    
     <!-- RViz -->
    <node pkg="rviz" type="rviz" name="$(anon rviz)" respawn="false" output="screen" args="-d $(find topp2)/rviz/2d_search_opti.rviz"/>


</launch>